{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "output": "static",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1.js" }
  ],
  "redirects": [
    { "source": "/(.*)", "destination": "/", "statusCode": 200 }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 300
    }
  }
}
